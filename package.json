{
  "name": "wallet-api",
  "version": "0.0.1",
  "description": "Demo Application for a wallet service API",
  "keywords": [
    "wallet",
    "api",
    "demo",
    "lendsqr"
  ],
  "engines": {
    "node": ">=22.16.0",
    "npm": ">=10.9.2"
  },
  "license": "MIT",
  "author": {
    "name": "Odunayo Mohammed",
    "email": "factman60@gmail.com"
  },
  "repository": "https://github.com/factman/WalletApi.git",
  "homepage": "https://github.com/factman/WalletApi#readme",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node .",
    "dev": "tsx --watch --env-file .env index.ts",
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "migrate": "dotenvx run -- knex migrate:latest",
    "migrate:rollback": "dotenvx run -- knex migrate:rollback",
    "migrate:up": "dotenvx run -- knex migrate:up",
    "migrate:down": "dotenvx run -- knex migrate:down",
    "seed": "dotenvx run -- knex seed:run"
  },
  "dependencies": {
    "@types/luxon": "^3.6.2",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "luxon": "^3.6.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.1",
    "resend": "^4.5.1",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.44.1",
    "@eslint/js": "^9.27.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/knex": "^0.15.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.24",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vitest/ui": "^3.1.4",
    "eslint": "^9.27.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "prettier": "^3.5.3",
    "sqlite3": "^5.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  }
}